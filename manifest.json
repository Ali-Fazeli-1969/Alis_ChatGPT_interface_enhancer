{
	"name": "Ali's ChatGPT interface enhancer",
	"version": "1.0",
	"manifest_version": 3,
	"browser_specific_settings": {
		"gecko": {
			"id": "noemail@nodomain.com"
		}
	},
	"permissions":
		["tabs", "scripting", "storage"],
	"host_permissions": ["*://chatgpt.com/*"],
	"background": {
		"scripts": ["bg.js"]
	},
	"content_scripts": [
		{
			"css": ["style.css"],
			"js": ["cs.js"],
			"matches": ["*://chatgpt.com/*"],
			"run_at": "document_end"
		}
	]
}
